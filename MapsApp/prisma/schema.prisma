datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
  previewFeatures = ["reactNative"]
}

model MarkerData {
  id        String   @id @default(cuid())
  latitude  Float
  longitude Float
}

model ImageData {
  id       String   @id @default(cuid())
  markerId String
  uri      String
}